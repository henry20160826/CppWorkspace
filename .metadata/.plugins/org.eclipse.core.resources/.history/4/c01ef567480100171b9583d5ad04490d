#include <iostream>
using namespace std;
class interval
{
public:
	interval()
	{
		startTime = 0;
		endTime = 0;
	}
	void set(int s, int e)
	{
		startTime = s;
		endTime = e;
	}
	int startTime;
	int endTime;
private:
};
class Cmp {
public:
    int val;
    bool operator()(const interval &t1, const interval &t2)
    {
        if(t1.endTime < t2.endTime)
            return true;
        return false;
    }
};
int main()
{
	int startTime[8] =
	{ 0, 1, 3, 3, 4, 5, 6, 8 };
	int endTime[8] =
	{ 6, 4, 5, 8, 7, 9, 10, 11 };
	interval task[8];
	for (int i = 0; i < 8; ++i)
	{
		task[i].set(startTime[i], endTime[i]);
	}

	return 0;
}
