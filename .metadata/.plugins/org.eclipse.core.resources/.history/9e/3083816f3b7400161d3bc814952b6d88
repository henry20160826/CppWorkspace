//解决重集的排列，组合问题，利用容斥原理
//关键：利用容斥原理
#include <iostream>
using namespace std;

class RepeatSet
{
	//重集
public:
	RepeatSet(int *aw, int *kw, int lenw)
	{
		len = lenw;
		a = aw;
		k = kw;
	}
	int getCounts(int num)
	{
		//得到重集中元素num的数量
		return 0;

	}
private:
	int len; //集合中不同元素种数
	int *a; //不同的元素
	int *k; //相同元素的个数
};

int main()
{
	cout << "Hello World" << endl; // prints Hello World
	return 0;
}
