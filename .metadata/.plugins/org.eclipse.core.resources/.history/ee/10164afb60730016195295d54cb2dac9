#include <iostream>
#include <string>
using namespace std;

int getKinds(string s[], int len)
{
	int kinds = 1;
	int i, j;
	for (i = 1; i < len; i++)
	{
		for (j = 0; j < i; j++)
		{
			if (s[i] == s[j])
			{
				break;
			}
		}
		if (i == j)
		{
			kinds++;
		}
	}
	return kinds;
}

int main()
{
	cout << "Hello World" << endl; // prints Hello World
	int label;
	int buys;
	char *p = new char[40];
	string *list = new string[buys];
	cin >> label >> buys;
	for (int i = 0; i < buys; i++)
	{
		cout >> p;
		list[i](p);
	}
	int kins = getKinds(list, buys);
	return 0;
}
