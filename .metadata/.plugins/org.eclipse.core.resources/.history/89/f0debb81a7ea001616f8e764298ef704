// Given two binary strings, return their sum (also a binary string).
//
//For example,
//a = "11"
//b = "1"
//Return "100".

#include <iostream>
using namespace std;

class Solution
{
public:
	string addBinary(string a, string b)
	{
		int alen = a.length();
		int blen = b.length();
		int i;
		if (alen > blen)
		{
			string rs(alen, 0);
		}
		else
		{
			string rs(blen, 0);
		}
		for (i = 1; alen - i >= 0 && blen - i >= 0; ++i)
		{

		}
	}
};

int main()
{
	cout << "Hello World" << endl; // prints Hello World
	return 0;
}
