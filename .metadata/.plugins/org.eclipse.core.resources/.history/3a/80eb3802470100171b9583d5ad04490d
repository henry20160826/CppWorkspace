#include <iostream>
using namespace std;
class interval
{
public:
	interval(int s, int e) :
			startTime(s), endTime(e)
	{
	}
	interval()
	{
		interval(0, 0);
	}

	int startTime;
	int endTime;
private:
};
int main()
{
	interval a[8];
	return 0;
}
