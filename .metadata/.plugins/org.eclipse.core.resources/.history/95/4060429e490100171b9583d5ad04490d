#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
class interval
{
public:
	interval()
	{
		startTime = 0;
		endTime = 0;
	}
	void set(int s, int e)
	{
		startTime = s;
		endTime = e;
	}
	int startTime;
	int endTime;
private:
};
class Cmp
{
public:
	int val;
	bool operator()(const interval &t1, const interval &t2)
	{
		if (t1.endTime < t2.endTime)
		{
			return true;
		}
		return false;
	}
};
#define N 8;
int main()
{
	//И│ох
	int startTime[N] =
	{	0, 1, 3, 3, 4, 5, 6, 8};
	int endTime[N] =
	{	6, 4, 5, 8, 7, 9, 10, 11};
	vector<interval> v;
	for (int i = 0; i < 8; ++i)
	{
		v.push_back(interval(startTime[i], endTime[i]));
	}
	sort(v.begin(), v.end(), Cmp());
	for (int i = 0; i < N; ++i
			)
			{
				cout << v[i].startTime << " " << v[i].endTime << endl;
			}
			return 0;
		}
